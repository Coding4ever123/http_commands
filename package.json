{
    "name": "http",
    "displayName": "http",
    "description": "send http requests",
    "version": "1.0.0",
    "publisher": "Adrian",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "type": "commonjs",
    "main": "./out/extension.js",
    "activationEvents": [
        "onCommand:Adrian.http.request"
    ],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "Adrian-Networking",
                    "title": "Networking",
                    "icon": "media/icon.svg"
                }
            ]
        },
        "views": {
            "Networking": [
                {
                    "type": "webview",
                    "id": "Adrian.networking.sidebar",
                    "name": "Networking",
                    "icon": "media/icon.svg",
                    "contextualTitle": "Networking"
                }
            ]
        },
        "commands": [
            {
                "command": "Adrian.http.request",
                "title": "Send http request"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -w -p ./"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.52.0",
        "glob": "^7.1.6",
        "mocha": "^10.4.0",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    },
    "dependencies": {
        "axios": "^1.7.2"
    }
}
