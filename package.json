{
    "name": "http",
    "displayName": "http",
    "description": "send http requests",
    "version": "2.2.0",
    "publisher": "Adrian",
    "repository": {
        "url": "https://github.com/Coding4ever123/http_commands"
    },
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "type": "commonjs",
    "main": "./out/extension.js",
    "activationEvents": [
        "onCommand:Adrian.http.request"
    ],
    "contributes": {
        "commands": [
            {
                "command": "Adrian.http.request",
                "title": "Send http request"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -w -p ./"
    },
    "devDependencies": {
        "@types/convert-units": "2.3.9",
        "@types/node": "14.18.10",
        "@types/superagent": "8.1.9",
        "@types/vscode": "1.52.0",
        "typescript": "4.1.2"
    },
    "dependencies": {
        "@xml-tools/parser": "^1.0.11",
        "convert-units": "2.3.4",
        "prettier": "3.3.3",
        "superagent": "10.1.0"
    }
}
