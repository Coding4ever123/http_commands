{
    "name": "http",
    "displayName": "http",
    "description": "send http requests",
    "version": "3.0.2",
    "publisher": "AdrianDoesCoding",
    "repository": "https://github.com/Coding4ever123/http_commands",
    "icon": "media/icon.png",
    "engines": {
        "vscode": "^1.95.0"
    },
    "categories": [
        "Testing",
        "Visualization"
    ],
    "type": "commonjs",
    "main": "./out/extension.js",
    "activationEvents": [],
    "contributes": {
        "commands": [
            {
                "command": "Adrian.http.request",
                "title": "Send http request",
                "category": "http"
            },
            {
                "command": "Adrian.http.fund",
                "title": "Fund",
                "category": "http"
            },
            {
                "command": "Adrian.http.speed",
                "title": "Test speed",
                "category": "http"
            },
            {
                "command": "Adrian.http.history",
                "title": "Show history",
                "category": "http"
            },
            {
                "command": "Adrian.http.clearhistory",
                "title": "Clear history",
                "category": "http"
            },
            {
                "command": "Adrian.http.cleartemp",
                "title": "Clear temprorary files",
                "category": "http"
            }
        ]
    },
    "funding": {
        "type": "github sponsors",
        "url": "https://www.github.com/sponsors/Coding4ever123"
    },
    "scripts": {
        "package": "vsce publish",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -w -p ./"
